<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <add name="Django Handler" path="*" verb="*" modules="FastCgiModule" scriptProcessor="C:\inetpub\wwwroot\App\env\Scripts\python.exe|C:\inetpub\wwwroot\App\env\Lib\site-packages\wfastcgi.py" resourceType="Unspecified" />
            <add name="Python FastCGI" path="*" verb="*" modules="FastCgiModule" scriptProcessor="C:\inetpub\wwwroot\App\env\Scripts\python.exe|C:\inetpub\wwwroot\App\env\Lib\site-packages\wfastcgi.py" resourceType="Unspecified" requireAccess="Script" />
        </handlers>
    </system.webServer>
    <appSettings>
        <!-- Required settings -->
        <add key="WSGI_HANDLER" value="App.wsgi.application" />
        <add key="PYTHONPATH" value="C:\inetpub\wwwroot\App" />
        <add key="DJANGO_SETTINGS_MODULE" value="App.settings" />
    </appSettings>
</configuration>